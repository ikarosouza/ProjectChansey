<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.imd.view.SectorFormController">
	<children>
      <VBox layoutX="10.0" layoutY="10.0" spacing="5.0">
         <children>
            <Label text="Cadastro de Setores">
               <font>
                  <Font name="Impact" size="29.0" />
               </font>
            </Label>
            <VBox prefHeight="172.0" prefWidth="626.0" spacing="5.0">
               <children>
                  <VBox spacing="5.0">
                     <children>
                        <Label text="Nome" />
                        <TextField fx:id="nameField" prefHeight="26.0" prefWidth="588.0" />
                     </children>
                  </VBox>
                  <HBox spacing="25.0">
                     <children>
                        <VBox spacing="5.0">
                           <children>
                              <Label text="Qtd. de leitos" />
                              <TextField fx:id="numBedsField" prefHeight="26.0" prefWidth="73.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleSaveSector" prefHeight="26.0" prefWidth="86.0" styleClass="button-new" text="Salvar" />
                  <Button mnemonicParsing="false" onAction="#handleCancel" prefHeight="26.0" prefWidth="86.0" styleClass="button-clean" text="Cancelar" />
                  <Button mnemonicParsing="false" onAction="#handleBack" styleClass="button-back" text="Voltar" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
